{
    "env": {
        "commonjs": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier",
        "import"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "off",
        "no-empty-function": "off",
        "import/extensions": [
            "error",
            {
                "js": "always",
                "json": "always"
            }
        ]
    }
}
